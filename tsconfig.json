{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/app/about-us/page.js", "src/app/engagement/page.js", "src/app/events-and-destinations/page.js", "src/app/fbos-and-mros/page.js", "src/app/networks/page.js", "src/app/real-estate-and-hospitality/page.js", "src/app/travel-and-lifestyle/page.js", "src/app/agents-and-advisors/page.js"  ],
  "exclude": [
    "node_modules"
  ]
}
